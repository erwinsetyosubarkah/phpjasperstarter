{
  "name": "erw/phpjasperstarter",
  "description": "A PHP 5.6 report generator",
  "license": "MIT",
  "keywords": [
    "reports",
    "php-reports",
    "pdf"
  ],
  "authors": [
    {
      "name": "Erwin Setyo S",
      "email": "kawanerror@gmail.com",
      "role": "lead"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^5|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "PHPJasper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPJasper\\Test\\": "tests/"
    }
  },
  "scripts": {
    "unit": "php56 vendor/bin/phpunit --coverage-clover ./tests/log/clover.xml --colors=always",
    "testdox": "phpunit --testdox",
    "test": [
      "@unit"
    ]
  }
}
